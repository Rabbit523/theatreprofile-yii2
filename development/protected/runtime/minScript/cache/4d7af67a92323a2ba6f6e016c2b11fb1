function link_adder(id)
{
	var random_hash_name='field_'+Math.floor((Math.random() * 10000) + 1);
	var html='<div class="well" id="'+random_hash_name+'"><a class="ui-icon ui-icon-close pull-right" onclick="$(this).parent().remove();"></a><label>URL</label><input type="text" class="input-block-level" name="links[new]['+cfg.link_counter+'][href]" required="required" /><label>Label</label><input type="text" class="input-block-level" name="links[new]['+cfg.link_counter+'][label]" required="required" value="Buy Tickets" /><input type="hidden" name="links[new]['+cfg.link_counter+'][productionVenueID]" value="'+id+'"/></div>';
	cfg.link_counter++;
	$("#ticketinglinks_"+id).append(html);
}

