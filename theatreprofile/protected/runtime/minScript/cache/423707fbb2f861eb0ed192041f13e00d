		ga('create', 'UA-3986808-1', { 'userId': cfg.userID,'cookieDomain': 'www.theatreprofile.com'});
	else
		ga('create', 'UA-3986808-1', {'cookieDomain': 'www.theatreprofile.com'});
	ga('send', 'pageview');
}
